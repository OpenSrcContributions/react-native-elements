{
  "expo": {
    "name": "React Native Elements",
    "owner": "rneui",
    "slug": "react-native-elements",
    "description": "A demo app for React Native Elements UI Library",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "4.0.0",
    "orientation": "default",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/icon.png",
    "scheme": "reactnativeelements",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rneui",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icons/icon.png",
        "backgroundImage": "./assets/icons/icon.png",
        "monochromeImage": "./assets/icons/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
